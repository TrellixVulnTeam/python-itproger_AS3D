num = int(input("Введите значения "))
bool = True
if num > 10:
    print("Переменная больше 10")
    if num == 50:
        print("И она равна 50")
    elif num > 50:
        print("даже больше 50")
elif num == 9:
    print("она равна 9")
else: #пишется в самом конце. не по середине
    print("переменная меньше 10 и не 9")

if num > 10 and bool: #'bool' = 'bool = True
    print('10 and True')

num1 = int(input("input "))
result = "Больше 5" if num1 > 5 else "Меньше"
print(result)